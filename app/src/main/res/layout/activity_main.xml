<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imglogodicord"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:scaleType="centerCrop"
        android:src="@drawable/logodiscord" />

    <ImageView
        android:layout_below="@+id/imglogodicord"
        android:id="@+id/imglogothreads"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:scaleType="centerCrop"
        android:src="@drawable/logothreads" />

    <ImageView
        android:layout_below="@+id/imglogothreads"
        android:id="@+id/imglogolinkedin"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:scaleType="centerCrop"
        android:src="@drawable/logolinkedin" />

    <ImageView
        android:layout_below="@+id/imglogolinkedin"
        android:id="@+id/imglogoreddit"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:scaleType="centerCrop"
        android:src="@drawable/logoreddit" />


</RelativeLayout>